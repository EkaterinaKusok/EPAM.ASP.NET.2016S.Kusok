@model IEnumerable<Day02.Task.ControllerFactory.Models.User>

@{
    ViewBag.Title = "Users list";
}

<p>
    @Html.ActionLink("Add new user", "Add-User")
</p>
<table class="table">
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
        </tr>
    }
</table>
